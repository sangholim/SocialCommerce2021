<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity Product.
    -->
    <changeSet id="20210530065727-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="product_notice_manage_id"
                                 baseTableName="product"
                                 constraintName="fk_product__product_notice_manage_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product_notice_manage"/>

        <addForeignKeyConstraint baseColumnNames="product_clazz_author_id"
                                 baseTableName="product"
                                 constraintName="fk_product__product_clazz_author_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product_clazz_author"/>

        <addForeignKeyConstraint baseColumnNames="product_store_id"
                                 baseTableName="product"
                                 constraintName="fk_product__product_store_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product_store"/>
    </changeSet>
</databaseChangeLog>
